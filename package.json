{
  "name": "typescript-gateway",
  "version": "1.0.0",
  "description": "Ultra-high-performance API gateway achieving sub-10ms P99 latency and 150k+ req/s throughput",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:perf": "vitest run tests/performance",
    "test:watch": "vitest",
    "benchmark": "node benchmarks/load-test.js",
    "benchmark:router": "tsx benchmarks/router-benchmark.ts",
    "benchmark:context": "tsx benchmarks/context-pool-benchmark.ts",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "api-gateway",
    "high-performance",
    "low-latency",
    "proxy",
    "router",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "ajv": "^8.12.0",
    "pino": "^8.16.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "autocannon": "^7.12.0",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "tsx": "^4.6.2",
    "typescript": "^5.3.2",
    "vitest": "^1.0.4"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
